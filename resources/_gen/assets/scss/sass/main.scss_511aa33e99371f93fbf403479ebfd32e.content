a{color:#00f;text-decoration:none}body{color:#222;font-family:sans-serif;font-size:16px;line-height:1.5;margin:0 1rem 3rem}h1{margin:.5rem 0}table{border-collapse:collapse}td,th{border:1px solid #222;padding:.1em .5em;vertical-align:top}th{background:#666;color:#fff;font-weight:400;text-align:left}.content{max-width:768px;min-height:1rem}.list-item{margin-bottom:.5rem}.header{display:flex;justify-content:space-between;align-items:center}.nav{list-style-type:none;padding-left:0;display:flex;margin:0}.nav-item{margin-right:.75rem}@media(max-width:576px){.last-updated,.types{display:none}}@media(max-width:768px){.version,.visibility{display:none}}@media(max-width:992px){.description{display:none}}