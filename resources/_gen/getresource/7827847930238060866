    const navBarBlur = document.getElementById('nav-bar-blur');

    function test() {
        let positionY = window.pageYOffset || document.documentElement.scrollTop;

        if (positionY > navBarBlur.style.height) {
            navBarBlur.classList.add('blur--active');
        } else {
            navBarBlur.classList.remove('blur--active');
        }
    }

    window.onscroll = test;
