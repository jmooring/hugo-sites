    <script>
        const popUp = document.getElementById('floating-credits');

        function popUpHandler(event, mode) {
            if (popUp.style.opacity === '0') {
                popUp.style.pointerEvents = "all";
                popUp.style.opacity = '1';
            } else {
                popUp.style.pointerEvents = "none";
                popUp.style.opacity = '0';
            }
        }

        popUp.addEventListener('click', e => {
            if (e.target === popUp) {
                popUpHandler();
            }
        })
    </script>
    <script>
        
        const urlParams = new URLSearchParams(window.location.search);


        
        const utm_medium = urlParams.get('utm_medium') || ""
        const utm_source = urlParams.get('utm_source') || ""
        const utm_campaign = urlParams.get('utm_campaign') || ""
        const utm_content = urlParams.get('utm_content') || ""
        const reserved_code_media = urlParams.get('reserved_code_media') || ""


        
        links = document.querySelectorAll(".dynamic_link");

        links.forEach(function(outboundLink) {
            
            if (utm_medium) outboundLink.href += "&utm_medium=" + utm_medium; 
            if (utm_source) outboundLink.href += "&utm_source=" + utm_source;
            if (utm_campaign) outboundLink.href += "&utm_campaign=" + utm_campaign;
            if (utm_content) outboundLink.href += "&utm_content=" + utm_content;
            if (reserved_code_media) outboundLink.href += "&reserved_code_media=" + reserved_code_media;
        });
    </script>

