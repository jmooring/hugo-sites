<script type="text/javascript">
   function validateForm() {
      var errorMessage = document.getElementById('error-message');
      var anliegen     = document.querySelector('input[name="anliegen"]:checked');
      var name         = document.querySelector('input[name="name"]');
      var email        = document.querySelector('input[name="email"]');
      var telefon      = document.querySelector('input[name="telefon"]');
      var confirm      = document.querySelector('input[name="confirm"]:checked');

      errorMessage.innerHTML = '';

      if (!anliegen) {
         errorMessage.innerHTML = "<p>Bitte f√ºll' alle erforderlichen Felder (*) aus.</p>";
         return false;
      } else {
      	 var anliegenVal  = anliegen.value;
         if (anliegenVal == 'Newsletter') {
            if (!name.value || !email.value || !confirm) {
               errorMessage.innerHTML = "<p>Bitte f√ºll' alle erforderlichen Felder (*) aus.</p>";
               return false;
            }
         } else {
            if (!name.value || !email.value || !telefon.value || !confirm) {
               errorMessage.innerHTML = "<p>Bitte f√ºll' alle erforderlichen Felder (*) aus.</p>";
               return false;
            }
         }
      }

      var emailRegex = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
      if (!emailRegex.test(email.value)) {
         errorMessage.innerHTML = "<p>Bitte gib' eine g√ºltige E-Mail-Adresse ein.</p>";
         return false;
      }

      return true;
   }   
</script> 


                
