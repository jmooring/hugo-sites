      <script src="https://evawilhelm.de/js/script.js"></script>
      
<script type="text/javascript">
let regForm  = document.getElementById("anmeldung").querySelector("form");
let telField = regForm.querySelector('input[name="telefon"]');


let regID = {
  "eb"  : "consultantNewsletters1",
  "pa1" : "consultantNewsletters1",
  "pa2" : "consultantNewsletters1",
  "fk1" : "consultantNewsletters2",
  "fk2" : "consultantNewsletters3",
  "nl"  : "consultantNewsletters5"
};


let regText = {
  "eb"  : "Ich bin interessiert an einer Einzelberatung.",
  "pa1" : "Ich bin interessiert an Paketangebot 1 (3 Termine).",
  "pa2" : "Ich bin interessiert an Paketangebot 2 (5 Termine).",
  "fk1" : "Ich bin interessiert am Frauenkreis Harburg.",
  "fk2" : "Ich bin interessiert am Frauenkreis Devah.",
  "nl"  : "Ich m√∂chte mich in den Newsletter eintragen."
};


regForm.querySelectorAll("input[type='radio']").forEach(item => {
  item.addEventListener('click', event => {
    let id = item.id;
    if (id == "consultantNewsletters5") {
      telField.style.display = "none";
    } else {
      telField.style.display = "block";
    }
  });
});


document.getElementById("konditionen").querySelectorAll("a").forEach(item => {
  item.addEventListener('click', event => {
    let id      = item.id;
    let chkID   = regID[id];
    let setText = regText[id];
    if (chkID) {
      document.getElementById(chkID).checked = true;
      if (setText) {
        regForm.querySelector("textarea").value = setText;
      }
      telField.style.display = "block";
    }
  });
});


let nlForm  = document.getElementById("newsletter");

nlForm.onsubmit = function() {
  let email   = nlForm.querySelector('input[type="email"]').value;
  let chkID   = regID["nl"];
  let setText = regText["nl"];
  if (chkID) {
    document.getElementById(chkID).checked = true;
  }
  if (setText) {
    regForm.querySelector("textarea").value = setText;
  }
  if (email != '') {
    regForm.querySelector('input[name="email"]').value = email;
  }
  telField.style.display = "none";
  document.getElementById("anmeldung").scrollIntoView();

  return false;
};
</script>


    </body>
</html>
