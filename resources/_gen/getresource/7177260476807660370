  <script>
      var _hsp = window._hsp = window._hsp || [];
      _hsp.push(['addPrivacyConsentListener', function (consent) {
        if (consent.categories.functionality) {
          let handoffData;
          let formattedText = "";
          let handoffTags = [];

          function initializeChatAnalytics() {
            if (typeof zE !== 'undefined') {
              zE("messenger", "logoutUser")
              
              function checkProperties(obj) {
                
                obj.forEach(item => {
                  
                  if (item.fromName === "bot") {

                    
                    if (item.kmResponse) {

                      
                      formattedText += `\n Bot: \n ${item.message}\n Article title: ${item.kmResponse.title}\n Article Url: ${item.kmResponse.url}\n`;

                    } else {
                      
                      formattedText += `\n Bot: \n ${item.message}\n`;
                    }

                    return;
                  } else if (item.fromName === "Consumer") {
                    formattedText += `\n Human: \n ${item.message}\n`;
                  }

                  for (const key in item) {
                    if (item.hasOwnProperty(key) && typeof item[key] === 'object') {
                      
                      checkProperties(item[key]);
                    } else {
                      
                      if (item.fromName === "Consumer") {
                        switch (item.message) {
                          case "Potential customer":
                              if (!handoffTags.includes('msg-persona-potential-customer')) {
                                  handoffTags.push("msg-persona-potential-customer");
                              }
                              break;
                          case "Current customer":
                              if (!handoffTags.includes('msg-persona-current-customer')) {
                                  handoffTags.push("msg-persona-current-customer");
                              }
                              break;
                          case "Partner or reseller":
                              if (!handoffTags.includes('msg-persona-partner')) {
                                  handoffTags.push("msg-persona-partner");
                              }
                              break;
                          case "End-user":
                              if (!handoffTags.includes('msg-persona-end-user')) {
                                  handoffTags.push("msg-persona-end-user");
                              }
                              break;
                          case "Student":
                              if (!handoffTags.includes('msg-persona-student')) {
                                  handoffTags.push("msg-persona-student");
                              }
                              break;
                          case "Someone else":
                              if (!handoffTags.includes('msg-persona-other')) {
                                  handoffTags.push("msg-persona-other");
                              }
                              break;
                          case "Sales or licensing help":
                              if (!handoffTags.includes('type-sales')) {
                                  handoffTags.push("type-sales");
                              }
                              break;
                          case "Technical help":
                              if (!handoffTags.includes('type-support')) {
                                  handoffTags.push("type-support");
                              }
                              break;
                          case "MF and NG":
                              if (!handoffTags.includes('product-mf-ng')) {
                                  handoffTags.push("product-mf-ng");
                              }
                              break;
                          case "Pocket, Hive and Views":
                              if (!handoffTags.includes('product-pocket-hive-views')) {
                                  handoffTags.push("product-pocket-hive-views");
                              }
                              break;
                          case "Job ticketing":
                              if (!handoffTags.includes('product-job-ticketing')) {
                                  handoffTags.push("product-job-ticketing");
                              }
                              break;
                          case "Mobility Print":
                              if (!handoffTags.includes('product-mobility-print')) {
                                  handoffTags.push("product-mobility-print");
                              }
                              break;
                          case "Print Deploy":
                              if (!handoffTags.includes('product-print-deploy')) {
                                  handoffTags.push("product-print-deploy");
                              }
                              break;
                          case "I am not sure":
                              if (!handoffTags.includes('product-not-sure')) {
                                  handoffTags.push("product-not-sure");
                              }
                              break;
                          default:
                              break;
                        }
                      }
                    }
                  }
                });
              }

              
              checkProperties(handoffData.detail.transcript);

              zE("messenger:set", "conversationTags", handoffTags);

              zE('messenger', 'open');

              handoffTranscript = formattedText;

              zE("messenger:set", "conversationFields", [
                { id: "8853350891791", value: handoffTranscript }
              ])
            } else {
              setTimeout(initializeChatAnalytics, 1000);
            }
          }

          window.addEventListener('support_handoff', (e) => {
            handoffData = e;
            var script = document.createElement('script');
            script.setAttribute("id", "ze-snippet");
            script.setAttribute("src", "https://static.zdassets.com/ekr/snippet.js?key=fa01d559-1bb5-4d6c-9feb-3d2fa8fba6cc");
            document.head.appendChild(script);
            window.zoomCampaignSdk.hide();
            initializeChatAnalytics();
          });
        }
      }]);
  </script>
  

  
  
