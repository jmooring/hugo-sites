<script defer>
  
  const checkContentCards = () => {
    if (contentCards && contentCards.length > 0) {
      
      const recommendedReadsCards = contentCards.filter(card => 
        card.extras && card.extras.blocks && card.extras.blocks.includes("c-card-layout--recommended")
      );
      processContentCards(recommendedReadsCards);
    } else {
      
      setTimeout(checkContentCards, 1000); 
    }
  };

  
  const processContentCards = (contentCards) => {
    const limit = 4; 
    const promises = []; 
    const cards = []; 
    let counter = 0; 

    
    const ctaCards = contentCards.filter(card => card.extras && card.extras.type === 'cta');
    let ctaCard = null;
    if (ctaCards.length > 0) {
      ctaCard = ctaCards.reduce((lowest, card) => 
        !lowest || parseInt(card.extras.weight) < parseInt(lowest.extras.weight) ? card : lowest, null);
    }

    
    if (ctaCard) {
      counter++;
      const newCard = createCardElement(ctaCard);
      cards.push(newCard);

      
      if (ctaCard.imageUrl) {
        const promise = loadImage(ctaCard.imageUrl)
          .then(thumbImg => {
            thumbImg.className = 'c-card-layout--recommended__recommended-cards__card__img';
            newCard.prepend(thumbImg);
          })
          .catch(error => console.error('Error loading image:', error));
        promises.push(promise);
      }
    }

    
    contentCards.forEach(card => {
      if (counter < limit && (!card.extras || card.extras.type !== 'cta' || card.id !== ctaCard.id)) {
        counter++;
        const newCard = createCardElement(card);
        cards.push(newCard);

        
        if (card.imageUrl) {
          const promise = loadImage(card.imageUrl)
            .then(thumbImg => {
              thumbImg.className = 'c-card-layout--recommended__recommended-cards__card__img';
              newCard.prepend(thumbImg);
            })
            .catch(error => console.error('Error loading image:', error));
          promises.push(promise);
        }
      }
    });

    
    Promise.all(promises)
      .then(() => {
        
        const placeholderCardsDiv = document.getElementById('placeholder-cards');
        placeholderCardsDiv.innerHTML = ''; 
        cards.forEach(card => {
          placeholderCardsDiv.appendChild(card);
        });

        
        cards.forEach(card => {
          const observer = new IntersectionObserver(entries => {
            entries.forEach(entry => {
              
              if (entry.isIntersecting && !entry.target.dataset.impressionLogged) {
                
                braze.logContentCardImpressions([card]);
                entry.target.dataset.impressionLogged = true; 
                observer.disconnect(); 
              }
            });
          }, { threshold: 0.5 }); 
          observer.observe(card);
        });
      })
      .catch(error => console.error('Error loading placeholder cards:', error));
  };

  
  const createCardElement = (card) => {
    const newCard = document.createElement('a');
    newCard.className = 'c-card-layout--recommended__recommended-cards__card';
    newCard.href = card.url || '#'; 
    newCard.dataset.cardId = card.id; 
    newCard.dataset.impressionLogged = false; 

    
    const cardTitle = document.createElement('p');
    cardTitle.className = 'c-card-layout--recommended__recommended-cards__card__title';
    cardTitle.textContent = card.title || '';
    newCard.appendChild(cardTitle);

    const cardLink = document.createElement('a'); 
    cardLink.className = 'c-card-layout--recommended__recommended-cards__card__link';
    cardLink.textContent = card.linkText || ''; 
    cardLink.href = (card.url || '#');
    newCard.appendChild(cardLink);

    
    newCard.addEventListener('click', () => handleCardClick(card)); 
    const observer = new IntersectionObserver(entries => {
      entries.forEach(entry => {
        if (entry.isIntersecting && !entry.target.dataset.impressionLogged) {
          
          braze.logContentCardImpressions([card]);
          entry.target.dataset.impressionLogged = true; 
          observer.disconnect(); 
        }
      });
    }, { threshold: 0.5 }); 
    observer.observe(cardLink);

    return newCard;
  };

  
  const handleCardClick = (card) => {
    event.preventDefault(); 

    
    if (braze.logContentCardClick(card)) {
      
      setTimeout(() => {
        window.location.href = card.url; 
      }, 5000); 
    } else {
      console.error("Error logging content card click.");
      
    }
  };

  
  checkContentCards();

  
  const loadImage = src =>
    new Promise((resolve, reject) => {
      const img = new Image();
      img.onload = () => resolve(img);
      img.onerror = reject;
      img.src = src;
    });
</script>
<!--bookshop-live end-->
		</div>
	
