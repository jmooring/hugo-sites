			if ( ! TCB_Front.isInViewport( $element ) ) {
				$at.removeClass( 'tve_anim_start' );
			}
		} );
	}
} else {
	setTimeout( function () {
		$at.addClass( 'tve_anim_start' );
	}, 50 );
}
return false;
