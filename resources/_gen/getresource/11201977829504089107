	return;
}
$at.data( 'a-done', 1 );
const hadAnimStart = $at.hasClass( 'tve_anim_start' );
$at.removeClass( function ( i, cls ) {
	return cls.split( ' ' ).filter( function ( item ) {
		return item.indexOf( 'tve_anim_' ) === 0;
	} ).join( ' ' );
} ).addClass( 'tve_anim_' + config.anim ).removeClass( 'tve_anim_start' );
