    <script>
        function loadMorePosts() {
            var items = document.querySelectorAll('.griditem');
            var counter = 0;
            items.forEach(function(item) {
                if(item.classList.contains('hidden') && counter < 16){
                    counter++;
                    item.classList.remove('hidden');
                }
            });
            if(!document.querySelectorAll('.griditem.hidden').length) {
                document.querySelector('.loadmore').remove();
            }
        }
    </script>
