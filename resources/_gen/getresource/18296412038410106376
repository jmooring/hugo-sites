<script>
window.addEventListener('load', () => {
  if(window.location.pathname == "/"){
    const HomeBody = document.getElementsByTagName('body')
    HomeBody[0].classList.add("homepage")
   
  }
 
});
    
    const navLinks = document.querySelectorAll(".nav-item");
    const baseUrlPath = window.location.pathname.split('/')[1];
    const innerBaseUrl = window.location.pathname.split('/')[2];
    const subMenuLinks = document.querySelectorAll('.innerNavLinks');
    const subMenuList = document.querySelectorAll('.inner-subMenus');
    navLinks.forEach(linkEl => {
      linkEl.classList.remove("active");
      
      linkEl.querySelectorAll("a").forEach(anchorEl => {
        if(baseUrlPath === ""){
        linkEl.classList.remove("active");
      }
        
        else if (anchorEl.getAttribute("href") && anchorEl.getAttribute("href").split("/")[1] === baseUrlPath) {
  
          linkEl.classList.add("active");
          subMenuList.forEach(item => {
            if (item.parentElement.getAttribute("id") === baseUrlPath) {
              item.parentElement.classList.add("hello");
              item.parentElement.classList.remove("abc");
              item.firstElementChild.classList.add('active');
            }
          })
        }
        if (baseUrlPath === 'categories') {
          subMenuList.forEach(item => {
            if (item.parentElement.getAttribute("id") === baseUrlPath) {
              item.parentElement.classList.add("hello");
              item.parentElement.classList.remove("abc");
              item.classList.add('active');
            }
            Array.prototype.forEach.call(item.children, child => {
              child.classList.remove('active')
              if (child.getAttribute("id") === innerBaseUrl) {
                child.classList.add('active')
                item.parentElement.classList.add("hello");
                item.parentElement.classList.remove("abc");
                if(anchorEl.innerHTML === "Resources"){
                  linkEl.classList.add('active');
                }
              }
            })
          })
        }
      })
    });
  </script>
</div></header>

<style>
  .abc {
    display: none !important;

  }

  .hello {
    display: block !important;
  }
</style>
