<script>
    let subNav = document.querySelectorAll(".nav_sub")
    var btn = document.querySelectorAll(".seclected-value");
    var btnSpan = document.querySelectorAll(".select");
    var dropdown = document.querySelectorAll(".inner-subMenus");
    
    var optionLinks = document.querySelectorAll(".inner-subMenus li");
    var storedOpt = localStorage.getItem('selectedVal');
    window.find_jobs_nav = function () {
        
        localStorage.setItem("selectedVal", "Matched");
    }
    

    window.Resources_nav = function () {
        
        localStorage.setItem("selectedVal", "Latest Articles")
    }
    var clickFn = function (e) {
        localStorage.removeItem('selectedVal')
        

        localStorage.setItem("selectedVal", e.target.innerText)
        
        btnSpan[0].innerHTML = e.target.innerText;
        

        
        
        
        
    };
    for (var i = 0; i < optionLinks.length; i++) {
        optionLinks[i].addEventListener("click", clickFn, false);
    }
    if (window.location.pathname === "/" || 
        window.location.pathname === "/jobs/" || 
        window.location.pathname === "/posts/privacy-policy/" || 
        window.location.pathname === "/posts/terms-and-conditions/" || 
        window.location.pathname === "/404.html") {
         subNav[0].style.display = "none";
    } else {
        subNav[0].style.display = "block";
        btnSpan[0].innerHTML = storedOpt;
        function btnClass() {
            btn[0].classList.toggle("select-clicked")
            dropdown.forEach(item => {
                if (item.parentElement.classList.contains('hello')) {
                    item.classList.toggle('open');
                }
                else if (!item.parentElement.classList.contains('hello')) {
                    localStorage.removeItem('selectedVal')
                    item.classList.toggle("open");
                }
            })
        };
    }
</script>

