<script>
    const mobileMenuWrapper = document.getElementsByClassName("open-menu-btn")[0];
    const threeDots = document.getElementById("open-menu-button");
    const closeMenuButton = document.getElementsByClassName("close-menu-btn")[0];
    const mobileMenuContainer = document.getElementsByClassName("mobile-menu-container")[0];
    mobileMenuWrapper.addEventListener("mouseenter", () => {
        threeDots.style.fill = "#8eacbb";
    });
    mobileMenuWrapper.addEventListener("mouseleave", () => {
        threeDots.style.fill = "rgb(255, 255, 255)";
    });
    mobileMenuWrapper.addEventListener("click", () => {
        mobileMenuContainer.className = "mobile-menu-container open";
    });
    closeMenuButton.addEventListener("click", () => {
        mobileMenuContainer.className = "mobile-menu-container";
    })
</script></nav>

