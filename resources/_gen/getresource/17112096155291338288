	<script type="text/javascript">
    try {
        let resource = "/biz/en-us";
        let parts = resource.split('/'); // split the url into parts
        let locale = parts[2] || ''; // get the locale part or use a default value
        let localeParts = locale.split('-');
        let formattedLocale = localeParts.length === 2 ? (localeParts[0] + '-' + localeParts[1].toUpperCase()) : ''; // ensure there are two parts to the locale
        if (formattedLocale == 'en-US') {
            /* Start Extole */
            (function(c,e,k,l,a){c[e]=c[e]||{};for(c[e].q=c[e].q ||[];a<l.length;)k(l[a++],c[e])})(window,"extole",function(c,e){e[c]=e[c]||function(){e.q.push([c,arguments])}},["createZone"],0);
            /* End Extole */

            extole.createZone({
                name: 'global_footer',
                element_id: 'extole_zone_global_footer',
                data: {
                    "locale": formattedLocale,
                    "email": "",
                }
            });

            extole.createZone({
                name: 'overlay',
                data: {
                    "locale": formattedLocale,
                    "partner_user_id": "",
                    "email": "",
                    "first_name": "",
                    "last_name": "",
                }
            });
            extole.createZone({
                name: 'mobile_menu',
                element_id: 'extole_zone_mobile_menu',
                data: {
                    "locale": formattedLocale,
                }
            });
        }
    } catch (err) {
        console.log('An error occurred:', err);
    }
</script>
