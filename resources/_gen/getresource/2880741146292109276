function loadAuditableZeroB3 () {
  if(window.innerWidth > 767) {
 
    var scrollPosition = window.scrollY
  
    var activeDiv = null
    for (var divId in divActivationPointsB3) {
      if (scrollPosition >= divActivationPointsB3[divId]) {
        activeDiv = divId
      }
    }
  
    if (activeDiv) {
      if (activeDiv != "line1_b3") {
  
        document.querySelector("#" + activeDiv + "").style.opacity = "1"
  
      } else {
          
        document.querySelector(".card__experience #line1_b3").style.height = "135px"
        setTimeout(function () {
        document.querySelector("#bloque1__titulo2_b3").style.opacity = '1' 
        }, 500)
        // ultimo bloque defense
        setTimeout(function () {
