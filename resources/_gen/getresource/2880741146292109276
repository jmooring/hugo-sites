          document.querySelector("#bloque1__titulo2_b3").style.opacity = "1"
        }, 500)
  
      } else if (activeTab == "tab4_b3" ) {
        // ultimo bloque defense
        document.querySelector(".card__experience #line2_b3").style.height = "180px"
        setTimeout(function () {
          document.querySelector("#div__bloque2_b3").style.opacity = "1"
          document.querySelector(".scroll-to-block").style.opacity = "1"
        }, 500)
  
      }
    } 


  
    if(  scrollPosition >= divActivationPointsB3['bloque1__titulo_b3'] ){
      if(document.querySelector("#bloque1__titulo2_b3").style.opacity == '0'){
        setTimeout(function () {
          document.querySelector("#bloque1__titulo2_b3").style.opacity = '1'
          document.querySelector(".card__experience #line2_b3").style.height = "180px"
          document.querySelector("#div__bloque2_b3").style.opacity = "1"
          document.querySelector(".scroll-to-block").style.opacity = "1"
        }, 500)
  
      }
      if(document.querySelector("#bloque1__titulo_b3").style.opacity == '0'){
         
        document.querySelector("#bloque1__titulo_b3").style.opacity = '1'
        document.querySelector("#line1_b3").style.height = '135px'
        document.querySelector(".card__experience #line2_b3").style.height = "180px"
        document.querySelector("#div__bloque2_b3").style.opacity = "1"
        document.querySelector(".scroll-to-block").style.opacity = "1"
        }
      
    }
  }
  
}

var zeroTrustWrapB3 = document.querySelector('#zero_trsut_feats1_b3')
if( zeroTrustWrapB3  ) {
   var zeroTrustLinksB3 =  zeroTrustWrapB3.querySelectorAll('.w-tab-link')
   
   zeroTrustLinksB3.forEach(function(zeroTrustLinkB3, index, array){

    zeroTrustLinkB3.addEventListener('mousedown',function(e){
        var idTabElement = event.target.id
        var limitScroll = tabActivationPointsB3[idTabElement] + 399
        console.log(idTabElement)
        console.log('limitScroll',limitScroll)
        $([document.documentElement, document.body]).scrollTop(limitScroll)
    })
   })
}

var scrolltoblock = document.querySelector('.scroll-to-block')
scrolltoblock.addEventListener('click',function(e){
   $([document.documentElement, document.body]).scrollTop(0);
   document.querySelector(".scroll-to-block").style.opacity = "0";
})



var divActivationPointsMobile={
  bloque1_1__mob: 400,
  bloque1_tit__mob: 684,
  tab1__mob: 1126, 
  line1__mob: 1340, 
  line2__mob: 1540, 
  line3__mob: 1740, 
  bloque1_3__mob: 2110,
  bloque2_1__mob: 2460, 
  bloque2_tit__mob: 2793, 
  tab5__mob: 3119, 
  line5__mob: 3353, 
  line6__mob: 3574, 
  line7__mob: 3853, 
  bloque2_3__mob: 4126,
  bloque3_1__mob: 4591, 
  bloque3_tit__mob: 4804, 
  tab9__mob: 5108, 
  line9__mob: 5388, 
  line10__mob: 5570, 
  bloque3_2__mob: 5760,
  bloque3_3__mob: 5900
}

var resaltarIcono ={ 
  tab1_ico: 1425,
  tab2_ico: 1665,
  tab3_ico: 1914,
  tab4_ico: 2146,
  tab5_ico: 3420,
  tab6_ico: 3680,
  tab7_ico: 3906,
  tab8_ico: 4153,
  tab9_ico: 4153,
  tab10_ico: 4153,
  tab11_ico: 4153
}

function loadMobile () {
  if(window.innerWidth <= 767) {
 

    var scrollPosition = window.scrollY
    console.log(scrollPosition)

    var activeDiv = null
    for (var divId in divActivationPointsMobile) {
      if (scrollPosition >= divActivationPointsMobile[divId]) {
        activeDiv = divId
      }
    }
  
    if (activeDiv) {

      if (activeDiv != 'tab1__mob' && activeDiv != 'line1__mob' && activeDiv != 'line2__mob' && activeDiv != 'line3__mob' 
        && activeDiv != 'tab5__mob' && activeDiv != 'line5__mob' && activeDiv != 'line6__mob' && activeDiv != 'line7__mob') {  
        document.querySelector("#" + activeDiv + "").style.opacity = "1" 
      }

      if (activeDiv == 'tab1__mob'){
        document.querySelector("#tab1__mob").style.opacity = '1'
        document.querySelector("#cont_text1__mob").style.opacity = '1'   
      }

      if (activeDiv == 'line1__mob'){
        document.querySelector("#line1__mob").style.height = '142px'
         setTimeout(function () {
          document.querySelector("#tab2__mob").style.opacity = '1'
          document.querySelector("#cont_text2__mob").style.opacity = '1'
        }, 500)
      }

      if (activeDiv == 'line2__mob'){
        document.querySelector("#line2__mob").style.height = '142px'
         setTimeout(function () {
          document.querySelector("#tab3__mob").style.opacity = '1'
          document.querySelector("#cont_text3__mob").style.opacity = '1'
        }, 500)
      }
      if (activeDiv == 'line3__mob'){
        document.querySelector("#line3__mob").style.height = '142px'
         setTimeout(function () {
          document.querySelector("#tabs4__mob").style.opacity = '1'
        }, 500)
      }

      if (activeDiv == 'tab5__mob'){
        document.querySelector("#tab5__mob").style.opacity = '1'
        document.querySelector("#cont_text5__mob").style.opacity = '1'   
      }

      if (activeDiv == 'line5__mob'){
        document.querySelector("#line5__mob").style.height = '142px'
         setTimeout(function () {
          document.querySelector("#tab6__mob").style.opacity = '1'
          document.querySelector("#cont_text6__mob").style.opacity = '1'
        }, 500)
      }


      if (activeDiv == 'line6__mob'){
        document.querySelector("#line6__mob").style.height = '142px'
         setTimeout(function () {
          document.querySelector("#tab7__mob").style.opacity = '1'
          document.querySelector("#cont_text7__mob").style.opacity = '1'
        }, 500)
      }

      if (activeDiv == 'line7__mob'){
        document.querySelector("#line7__mob").style.height = '168px'
         setTimeout(function () {
          document.querySelector("#tabs8__mob").style.opacity = '1'
        }, 500)
      }

      if (activeDiv == 'tab9__mob'){
        document.querySelector("#tab9__mob").style.opacity = '1'
        document.querySelector("#cont_text9__mob").style.opacity = '1'   
      }

      if (activeDiv == 'line9__mob'){
        document.querySelector("#line9__mob").style.height = '167px'
         setTimeout(function () {
          document.querySelector("#tab10__mob").style.opacity = '1'
          document.querySelector("#cont_text10__mob").style.opacity = '1'
        }, 500)
      }

      if (activeDiv == 'line10__mob'){
        document.querySelector("#line10__mob").style.height = '142px'
         setTimeout(function () {
          document.querySelector("#tabs11__mob").style.opacity = '1'
        }, 500)
      }



    }


    var activeBorde = null;

  // Verifica la posición actual del scroll en relación con las posiciones definidas
  for (var divId in resaltarIcono) {
    if (scrollPosition >= resaltarIcono[divId]) {
      activeBorde = divId;
    }
  }


  // Elimina la clase 'ico_active' de todos los elementos antes de agregarla al activo
  for (var divId in resaltarIcono) {
    document.querySelector("#" + divId).classList.remove("ico_active");
  }

  // Agrega la clase 'ico_active' al elemento activo, si hay uno
  if (activeBorde) {
    document.querySelector("#" + activeBorde).classList.add("ico_active");
  }




}//validacion

}



window.addEventListener("scroll", () => {
  loadAuditableZero();
  loadAuditableZeroB2();
  loadAuditableZeroB3();
  loadMobile();
})
window.addEventListener('load', () => {
  loadAuditableZero();
  loadAuditableZeroB2();
  loadAuditableZeroB3();
  loadMobile();
})

