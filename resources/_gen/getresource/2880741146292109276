        }, 500)
        setTimeout(function () {
          document.querySelector("#div__bloque2_b3").style.opacity = "1"
          document.querySelector(".scroll-to-block").style.opacity = "1"
        }, 1000)
        
      }
    }

    // Obtener la pestaña activa actualmente
    var activeTab = null
    for (var tabId in tabActivationPointsB3) {
      if (scrollPosition >= tabActivationPointsB3[tabId]) {
        activeTab = tabId
      }
    }

