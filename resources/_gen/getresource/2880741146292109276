var divActivationPoints = {
  bloque1__titulo: 469,
  line1: 474,
  zero_trsut_feats1: 507
} // Puntos para activar cada div
var tabActivationPoints = {
  tab1: 890,
  tab2: 1290,
  tab3: 1690,
  tab4: 2090,
  tab5: 2490
} // Puntos para activar cada tab

var limitSizeIndicator = 399
var sizeIndicator = 0

function loadAuditableZero () {

if(window.innerWidth > 767) {
    var scrollPosition = window.scrollY
    console.log(scrollPosition)
    
     if(scrollPosition < 964 ){
       document.querySelector(".scroll-to-block").style.opacity = "0"
    }
    
    
    
    var activeDiv = null
    for (var divId in divActivationPoints) {
      if (scrollPosition >= divActivationPoints[divId]) {
        activeDiv = divId
      }
    }
  
    if (activeDiv) {         
      if (activeDiv != "line1") {
        document.querySelector("#" + activeDiv + "").style.opacity = "1"
  
      } else {
          
        document.querySelector(".card__experience #line1").style.height = "135px"
        setTimeout(function () {
        document.querySelector("#bloque1__titulo2").style.opacity = '1'
         }, 500)

        // ultimo bloque defense
        setTimeout(function () {
        document.querySelector(".card__experience #line2").style.height = "180px"
        }, 500)
        setTimeout(function () {
          document.querySelector("#div__bloque2").style.opacity = "1"
        }, 1000)
        
      }
    }
  
    // Obtener la pestaña activa actualmente
    var activeTab = null
    for (var tabId in tabActivationPoints) {
      if (scrollPosition >= tabActivationPoints[tabId]) {
        activeTab = tabId
      }
    }
  
    // Simular clic en el enlace correspondiente a la pestaña activa
    if (activeTab) {
      if (activeTab != "tab5") {
        // item 1 a 4
        var tabLink = document.querySelector("#" + activeTab + "")
        console.log()
       if(!tabLink.classList.contains("w--current")){
            tabLink.click()
        }
        
        var startedPointTab = tabActivationPoints[activeTab]
        console.log("startedPointTab"+startedPointTab);
  
        if(scrollPosition >= startedPointTab) {
          sizeIndicator = scrollPosition - startedPointTab
          console.log("sizeIndicator"+sizeIndicator);
          if(sizeIndicator <= limitSizeIndicator){
              tabLink.querySelector('.indicator_item_tab').style.width = ((sizeIndicator * 100) / 399) +'%'

          }
  
        }
  

        setTimeout(function () {
          document.querySelector("#bloque1__titulo2").style.opacity = "1"
        }, 500)
  
      } 

    } 
  
    if(  scrollPosition >= divActivationPoints['bloque1__titulo'] ){
      if(document.querySelector("#bloque1__titulo2").style.opacity == '0'){
        setTimeout(function () {
          document.querySelector("#bloque1__titulo2").style.opacity = '1'
          document.querySelector(".card__experience #line2").style.height = "180px"
          document.querySelector("#div__bloque2").style.opacity = "1"
        }, 500)
        
  
      }
      if(document.querySelector("#bloque1__titulo").style.opacity == '0'){
         
        document.querySelector("#bloque1__titulo").style.opacity = '1'
        document.querySelector("#line1").style.height = '135px'        
        document.querySelector(".scroll-to-block").style.opacity = "0"
        }
        
    }

    

  } 

}

var zeroTrustWrap = document.querySelector('#zero_trsut_feats1')
if( zeroTrustWrap  ) {
   var zeroTrustLinks =  zeroTrustWrap.querySelectorAll('.w-tab-link')
   
   zeroTrustLinks.forEach(function(zeroTrustLink, index, array){

    zeroTrustLink.addEventListener('mousedown',function(e){
        var idTabElement = event.target.id
        var limitScroll = tabActivationPoints[idTabElement] + 399
        console.log(idTabElement)
        console.log('limitScroll',limitScroll)
        // window.scrollTo(0, limitScroll);
        $([document.documentElement, document.body]).scrollTop(limitScroll)
    })
   })
}


