                  window.dataLayer.push({'event': 'logEvent', 'eventType': 'newsletter subscribed', 'eventProperties': {'page name': 'VMware Tanzu Developer Center', 'source': 'footer'} });
                  sendAmplitudeEventOnLoad('newsletter subscribed', {'page name': 'VMware Tanzu Developer Center', 'source': 'footer', 'url path': window.location.pathname});
                  $('.confirmation').show();
                  return false;
                });
              });
            </script>
            <div class='confirmation' style="display:none">Done!</div>
          </div>
        
      </div>

    </div>
  </div>
</div>
</footer>


