Sentry.init({
  dsn: "https://91138cf7f17842fd8a57d45372bf538c@o448817.ingest.sentry.io/5712977",
  integrations: [new Sentry.Integrations.BrowserTracing()],
  release: 'ceb6d87e15b919e62ddac1d0ee73c7d106088576',
