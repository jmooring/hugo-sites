<script>
  

var player;


function onYouTubePlayerAPIReady() {
  
  player = new YT.Player('video', {
    events: {
      
      onReady: onPlayerReady
    }
  });
}

function onPlayerReady(event) {
  
  const playButton = document.querySelector('.video-trigger')
  const secondaryPlayButton = document.querySelector('.secondary-video-trigger')

  playButton.addEventListener('click', playVideo)
  secondaryPlayButton.addEventListener('click', playVideo)

  function playVideo () {
    player.playVideo()
  }

  const pauseButton = document.querySelector('.video-overlay__button')
  pauseButton.addEventListener('click', function () {
    player.pauseVideo()
  });
}


var tag = document.createElement('script')
tag.src = '//www.youtube.com/player_api'
var firstScriptTag = document.getElementsByTagName('script')[0]
firstScriptTag.parentNode.insertBefore(tag, firstScriptTag)

