.parentNode.style.display="block";let t=e.querySelector(".category-icon");t!==null&&(t.classList.remove("icon-right"),t.classList.add("icon-down")),e=e.parentNode.closest("li[data-nav-id]")}}$(".sidenav .category-icon").on("click",function(){return $(this).toggleClass("icon-down icon-right"),$(this).parent().parent().children("ul").slideToggle(200),!1}),$('[data-toggle="sidebar"]').on("click",function(){$(".sidenav").toggleClass("show")}),$(".sidenav").on("click",".sidenav-toggler",function(){return $(".sidenav").toggleClass("sidenav-hidden"),$("body").toggleClass("sidenav-invisible"),$(".content-block").toggleClass("mx-auto"),localStorage.setItem("sidenav","hidden"),!1}),localStorage.getItem("sidenav")=="hidden"&&($(".sidenav").addClass("sidenav-hidden"),$("body").addClass("sidenav-invisible"),$(".content-block").addClass("mx-auto"),$(".sidenav").on("click",".sidenav-toggler",function(){localStorage.removeItem("sidenav")})),$(".scrollmenu #TableOfContents li").length>0&&new ScrollMenu(".scrollmenu #TableOfContents a",{duration:400,activeOffset:40,scrollOffset:10}),$(".blog-sidebar #TableOfContents li").length>0&&new ScrollMenu(".blog-sidebar #TableOfContents a",{duration:400,activeOffset:200,scrollOffset:80});function o(){var e=$(window).width();e>1200&&$(".navbar-nav").find(".dropdown-menu").each(function(){$(this).height($(this).height())}),e<1200&&$(".navbar-nav").find(".dropdown-menu").each(function(){$(this).css("height","auto")})}o(),$(window).resize(function(){o()});function a(){$(".navbar-toggler").on("click",function(){$(this).children("i").toggleClass("d-inline d-none")})}if(a(),$('[data-bs-target="#videoModal"]').length!==0&&($('[data-bs-target="#videoModal"]').click(function(){e=$(this).data("src")}),$("#videoModal").on("shown.bs.modal",function(){$("#showVideo").attr("src",e+"?autoplay=1&amp;modestbranding=1&amp;showinfo=0")}),$("#videoModal").on("hide.bs.modal",function(){$("#showVideo").attr("src",e)})),$(".counter").length!==0){var e,t,i=0;$(window).scroll(function(){var e=$(".counter").offset().top-window.innerHeight;i==0&&$(window).scrollTop()>e&&($(".counter").each(function(){var e=$(this),t=e.attr("data-count");$({countNum:e.text()}).animate({countNum:t},{duration:850,easing:"swing",step:function(){e.text(Math.ceil(this.countNum).toLocaleString("en"))},complete:function(){e.text(Math.ceil(this.countNum).toLocaleString("en"))}})}),i=1)})}$(".brand-carousel").length!==0&&new Swiper(".brand-carousel.swiper-container",{speed:400,loop:!0,grabCursor:!0,autoplay:!0,pagination:{el:".swiper-pagination",type:"bullets",clickable:!0},breakpoints:{320:{slidesPerView:2},480:{slidesPerView:3},767:{slidesPerView:4},991:{slidesPerView:5}}}),$(".tab-content").find(".tab-pane").each(function(){var n=$(this).closest(".code-tabs").find(".nav-tabs"),s=$(this).attr("title");n.append('<li class="nav-item"><a class="nav-link" href="#">'+s+"</a></li>")}),$(".code-tabs ul.nav-tabs").each(function(){$(this).find("li:first").addClass("active")}),$(".code-tabs .tab-content").each(function(){$(this).find("div:first").addClass("active")}),$(".nav-tabs a").click(function(e){e.preventDefault();var t=$(this).parent(),s=t.index(),n=$(this).closest(".code-tabs"),o=n.find(".tab-pane").eq(s);n.find(".active").removeClass("active"),t.addClass("active"),o.addClass("active")}),$(".collapse").on("shown.bs.collapse",function(){$(this).parent().find(".fas fa-plus").removeClass("fas fa-plus").addClass("fas fa-minus")}).on("hidden.bs.collapse",function(){$(this).parent().find(".fas fa-minus").removeClass("fas fa-minus").addClass("fas fa-plus")}),new Swiper(".post-carousel.swiper-container",{speed:400,slidesPerView:1,autoplay:!0,loop:!0,pagination:{el:".swiper-pagination",type:"bullets",clickable:!0}}),new Swiper(".testimonials-carousel.swiper-container",{speed:400,loop:!0,grabCursor:!0,autoHeight:!0,autoplay:!0,pagination:{el:".swiper-pagination",type:"bullets",clickable:!0}}),t=$(".process-item"),t.each(function(){var e=$(this);$(window).on("scroll",function(){e.offset().top-$(window).scrollTop()<$(window).outerHeight()-250&&e.find(".image-block").addClass("active")})}),$(window).on("scroll",function(){$(window).scrollTop()===0&&t.find(".image-block").removeClass("active")}),$(".infinite-scroll").infiniteScroll({path:".nextpage",append:".blog-card",history:!1,status:".page-load-status"})}),$(window).on("load",function(){$("div.navbar-right").hasClass("platform-button")&&$("#secondary-button").hide()}),$(".navigation-alt").length!==0){let n=$(".navigation-alt"),e=$(".navigation-bottom"),s=n.outerHeight(),t=e.outerHeight();e&&$(window).on("scroll",function(){$(window).scrollTop()>(s+t)*2?(e.addClass("sticky"),e.css("top",0-t+"px")):(e.removeClass("sticky"),e.css("top","unset"))})}</script>










