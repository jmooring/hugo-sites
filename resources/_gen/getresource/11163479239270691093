  focusNextSearchResult(){
    if(this.focusedResultIndex == null && this.$refs.searchResultList.children.length > 0){
      this.focusedResultIndex = 0;
      this.focusedResultID = 'search-result-link-' + this.focusedResultIndex;
    }
    else if(this.focusedResultIndex > this.$refs.searchResultList.children.length){
      this.focusedResultIndex = 0;
      this.focusedResultID = 'search-result-link-' + this.focusedResultIndex;
    }
    else if((this.focusedResultIndex + 1 ) < this.$refs.searchResultList.children.length - 1 ){
      this.focusedResultIndex++;
      this.focusedResultID = 'search-result-link-' + this.focusedResultIndex;
    }
    else{
      this.focusedResultIndex = 0;
    }
    if(this.focusedResultID != null){
      this.focusedResultLink = document.getElementById(this.focusedResultID).href;
    }

  },
  
  focusPrevSearchResult(){
    if(this.focusedResultIndex == null && this.$refs.searchResultList.children.length > 0){
      this.focusedResultIndex = this.$refs.searchResultList.children.length - 1;
      this.focusedResultID = 'search-result-link-' + this.focusedResultIndex;

    }

    else if(this.focusedResultIndex > this.$refs.searchResultList.children.length){
      this.focusedResultIndex = 0;
      this.focusedResultID = 'search-result-link-' + this.focusedResultIndex;
    }

    else if((this.focusedResultIndex - 1 ) >= 0  ){
      this.focusedResultIndex--;
      this.focusedResultID = 'search-result-link-' + this.focusedResultIndex;
    }
    else{
      this.focusedResultIndex = this.$refs.searchResultList.children.length - 2;
    }
    
    if(this.focusedResultID != null){
      this.focusedResultLink = document.getElementById(this.focusedResultID).href;
    }

  },

  
  searchUpdateReset(){
    if(this.focusedResultIndex != null){
        this.resetDropDownValues();
    }

  },
  
  resetDropDownValues(){
    this.focusedResultIndex = null;
    this.focusedResultID = null;
    this.focusedResultLink= null;
  }
