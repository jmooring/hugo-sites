    <section id="cookie-policy-disclaimer">
      <span id="accept-cookies" class="close-button">&times;</span>
      <h2>Important information regarding cookies and phxlabs.ca</h2>
<p>By using this website, you consent to the use of cookies in accordance with the <a href="/privacy-policy/#cookies">Phoenix Labs Cookie Policy</a>. For more information on cookies and how to make choices regarding them, see our <a href="/privacy-policy/#cookies">cookie policy</a>.</p>
    </section>
    <script>
      var cookies = {
          version: "2019-11-20 10:00:00 PST",
          set: function (cookie_name, cookie_value, expires) {
            var cookie_expiry = "";
            if (!!expires) {
              var expiresUTC = new Date(expires);
              cookie_expiry = "expires=" + expiresUTC.toUTCString() + ";";
            } else {
              var plusTwoYears = new Date();
              plusTwoYears.setTime(plusTwoYears.getTime() + (730 * 24 * 60 * 60 * 1000));
              cookie_expiry = "expires=" + plusTwoYears.toUTCString() + ";";
            }
            document.cookie = cookie_name + "=" + cookie_value + ";" + cookie_expiry + "path=/; domain=" + location.hostname + "; SameSite=Lax";
          },
          get: function (cookie_name) {
            cookie_name = cookie_name + "=";
            var decodedCookie = decodeURIComponent(document.cookie);
            var ca = decodedCookie.split(";");
            var i, c;
            for (i = 0; i < ca.length; i += 1) {
              c = ca[i];
              while (c.charAt(0) === " ") {
                c = c.substring(1);
              }
              if (c.indexOf(cookie_name) === 0) {
                return c.substring(cookie_name.length, c.length);
              }
            }
            return "";
          },
          remove: function (cookie_name) {
            document.cookie = cookie_name + "=;expires=Thu, 01 Apr 2014 00:00:01 GMT;path=/;domain=" + location.hostname;
            document.cookie = cookie_name + "=;expires=Thu, 01 Apr 2014 00:00:01 GMT;path=/;";
          }
        };
      var cookie_disclaimer = document.getElementById("cookie-policy-disclaimer");
      var accept_cookies = document.getElementById("accept-cookies");
      cookie_disclaimer.style.display = cookies.get("cpv") !== cookies.version ? 'block' : 'none';
      accept_cookies.onclick = function () {
        cookie_disclaimer.style.display = 'none';
        cookies.set('cpv', cookies.version);
      };
    </script>

    
    
    
    
    
    
    
    
    
    
    
