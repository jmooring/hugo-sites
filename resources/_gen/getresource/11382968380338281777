				<div class="et_pb_code_inner"><script>
jQuery(document).ready(function($) {
    // Intercept the submit event of the first form
    $('.et_pb_contact_form').on('submit', function(event) {
        event.preventDefault(); // Prevent the default form submission

        // Extract data from the first form
        var firstName = $('#et_pb_contact_first_name_0').val() || $('#et_pb_contact_first_name_1').val();
        var lastName = $('#et_pb_contact_last_name_0').val() || $('#et_pb_contact_last_name_1').val();
        var companyName = $('#et_pb_contact_company_name_0').val() || $('#et_pb_contact_company_1').val();
        var title = $('#et_pb_contact_title_0').val() || $('#et_pb_contact_title_1').val();
				var emailAddress = $('#et_pb_contact_email_address_0').val() || $('#et_pb_contact_email_address_1').val();
        var phoneNumber = $('#et_pb_contact_phone_number_0').val() || $('#et_pb_contact_phone_1').val();
        var country = $('#et_pb_contact_country_0').val() || $('#et_pb_contact_country_1').val();
        var state = $('#et_pb_contact_state_0').val() || $('#et_pb_contact_state_1').val();
        var interestedIn = [];
        var pagetitle = $('#top h1').text();
        // Fetch the current page URL
        var currentPageURL = window.location.href;
        // Replace "dev.webscale.cc" or "dev.webscale.com" with "www.webscale.com" in the URL
        var modifiedPageURL = currentPageURL.replace(/dev\.webscale\.(cc|com)/, "www.webscale.com");
        // Output the modified URL
        console.log("Modified Page URL:", modifiedPageURL);

        // Extract selected checkboxes from the first form
        $('.et_pb_contact_field_10 .et_pb_contact_field_checkbox input[type="checkbox"]:checked').each(function() {
            interestedIn.push($(this).val());
        });

        // Populate the fields in the second form
        $('#et_pb_signup_firstname').val(firstName);
        $('#et_pb_signup_lastname').val(lastName);
        $('#et_pb_contact_8_2').val(companyName);
        $('#et_pb_contact_6_2').val(title);
				$('#et_pb_signup_email').val(emailAddress);
        $('#et_pb_contact_4_2').val(phoneNumber);
        $('#et_pb_contact_9_2').val(country);
        $('#et_pb_contact_10_2').val(state);
        $('#et_pb_contact_11_2').val(interestedIn.join(', '));
$('#et_pb_contact_13_2').val(pagetitle);
$('#et_pb_contact_12_2').val(modifiedPageURL);
        // Trigger the click event of the subscribe button in the second form
        $('.et_pb_newsletter_button').trigger('click');
    });
});

</script></div>
