<script type="rocketlazyloadscript">
var prevScrollpos = window.pageYOffset;
jQuery(window).on('scroll', function() {
  var currentScrollPos = window.pageYOffset;
  if (prevScrollpos > currentScrollPos && currentScrollPos > 450) {
    jQuery('.sticky-logo').css('opacity', 1).css('top', '61px');
  } else {
		if (currentScrollPos <= 450) {
			jQuery('.sticky-logo').css('opacity', 0);
  	} else {
			jQuery('.sticky-logo').css('top', '-38px');
		}
  }
  prevScrollpos = currentScrollPos;
});
</script>

<style>
.sticky-logo {
	position: fixed;
	top: -38px;
	width: 100%;
	transition: top .4s ease, opacity .2s !important;
}
.admin-bar .sticky-logo .elementor-widget-image {
	margin-top: 31px;
}
</style>
