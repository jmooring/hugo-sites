      const slide = document.createElement("div");
      slide.style.backgroundImage = `url(${item.image})`
      if (item.caption){
        const caption = item.captionLink ? document.createElement("a") : document.createElement("p");
        caption.classList.add('caption');
        caption.style.cssText = "bottom: 0px;";
        caption.innerHTML = item.caption;
        if (item.captionLink){
          caption.href = item.captionLink
          caption.target = "_blank"
        }
        slide.appendChild(caption)
      }


      
      let a = parseInt("3000", 10);
      let b = parseInt("1500", 10);
      let n = TOML.parse("\n\n  [[items]]\n  image = \u0022https:\/\/www.rhino3d.com\/8\/v8-lift-foils.png\u0022\n  caption = \u0022Lift Foils\u0022\n  captionLink = \u0022https:\/\/liftfoils.com\u0022\n  [[items]]\n  image = \u0022https:\/\/www.rhino3d.com\/8\/v8-surf-foil.png\u0022\n  caption = \u0022Kyle Houchens\u0022\n  captionLink = \u0022https:\/\/discourse.mcneel.com\/u\/theoutside\/activity\/portfolio\u0022\n  [[items]]\n  image = \u0022https:\/\/www.rhino3d.com\/8\/v8-amp.png\u0022\n  caption = \u0022Kyle Houchens\u0022\n  captionLink = \u0022https:\/\/discourse.mcneel.com\/u\/theoutside\/activity\/portfolio\u0022\n  [[items]]\n  image = \u0022https:\/\/www.rhino3d.com\/8\/v8-bike.png\u0022\n  caption = \u0022Kyle Houchens\u0022\n  captionLink = \u0022https:\/\/discourse.mcneel.com\/u\/theoutside\/activity\/portfolio\u0022\n  [[items]]\n  image = \u0022https:\/\/www.rhino3d.com\/8\/v8-ring.png\u0022\n  caption = \u0022Ring by T Shinya\u0022\n  captionLink = \u0022https:\/\/discourse.mcneel.com\/u\/t.shinya\/activity\/portfolio\u0022\n  [[items]]\n  image = \u0022https:\/\/www.rhino3d.com\/8\/v8-shoe.png\u0022\n  caption = \u0022Kyle Houchens\u0022\n  captionLink = \u0022https:\/\/discourse.mcneel.com\/u\/theoutside\/activity\/portfolio\u0022\n  [[items]]\n  image = \u0022https:\/\/www.rhino3d.com\/8\/v8-carapacks.png\u0022\n  caption = \u0022University of Applied Science Biberach, Simon Vorhammer\u0022\n  captionLink = \u0022https:\/\/discourse.mcneel.com\/t\/carapacks-pavillon\/165506\u0022\n  [[items]]\n  image = \u0022https:\/\/www.rhino3d.com\/8\/v8-bus.png\u0022\n  caption = \u0022Kyle Houchens\u0022\n  captionLink = \u0022https:\/\/discourse.mcneel.com\/u\/theoutside\/activity\/portfolio\u0022\n  [[items]]\n  image = \u0022https:\/\/www.rhino3d.com\/8\/v8-louisiana-sports-trahan.png\u0022\n  caption = \u0022Louisiana State Museum, Trahan Architects. Photo: Timothy Hursley\u0022\n  captionLink = \u0022https:\/\/trahanarchitects.com\/\u0022\n  [[items]]\n  image = \u0022https:\/\/www.rhino3d.com\/8\/v8-motorcycle.png\u0022\n  caption = \u0022Concept Bike by SabinoDesign\u0022\n  captionLink = \u0022https:\/\/discourse.mcneel.com\/t\/bike-concept\/139147\u0022\n\n").items.length;
      let t = (a+b)*n;

      var style = document.createElement('style');
      style.type = 'text/css';
      var id = "0";
      var keyFrames = `\
      @-keyframes fade-${id} {\
          0% {\
            opacity: 1;\
            pointer-events: all;\
          }\
          ${a/t * 100}% {\
            opacity: 1;\
            pointer-events: all;\ 
          }\
          ${1/n * 100}% {\
            opacity: 0;\
            pointer-events: none;\
          }\
          ${100 - (b/t * 100)}% {\
            opacity: 0;\
            pointer-events: none;\
          }\
          100% {\
            opacity: 1;\
            pointer-events: all;\
          }\
      }\
      @-moz-keyframes fade-${id} {\
        0% {\
          opacity: 1;\
          pointer-events: all;\
        }\
        ${a/t * 100}% {\
          opacity: 1;\
          pointer-events: all;\ 
        }\
        ${1/n * 100}% {\
          opacity: 0;\
