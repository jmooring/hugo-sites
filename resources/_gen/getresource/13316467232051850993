  <script>

    $(window).on('pageshow', function(){
        var showSegment = function() { 
            if($('#segmentA').is(':checked')){
                $('#pricing-new').css('display', '');
                $('#pricing-upgrade').css('display', 'none');
              } else {
                  $('#pricing-new').css('display', 'none');
                  $('#pricing-upgrade').css('display', '');
              }
        };
        showSegment();
      $('.segmentInput').change(function(){
        showSegment();
      });
    });
    </script>
