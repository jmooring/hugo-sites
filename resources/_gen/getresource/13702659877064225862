                                                <h3 class="uk-h2" id="veranstaltungen">Aktuelle Veranstaltungen</h3>
                                                <ul class="uk-grid uk-child-width-1-2@s uk-child-width-1-3@m uk-margin-medium-bottom" id="mev-content" uk-grid>
<script src="https://www.mellifera.de/js-pool/extern-content.js"></script>
<script>
    var mevContainer = document.getElementById("mev-content");

    
    fetch('/event/bienenkiste.json')
        .then(response => response.json())
        .then(results => {
            if (results.length === 0) {
                mevContainer.innerHTML = '<p>Derzeit gibt es keine aktuellen Termine.</p>';
            } else {
                var mevOutput = '';
                
                for (var i = 0; i < Math.min(results.length, 6); i++) {
                    var event = results[i];
                    var date = event.datum;
                    var title = event.summary;
                    var introtext = event.introtext;
                    var permalink = 'https://bienenkiste.de/event/' + event.id;
                    var location = event.location;

                    mevOutput += '<li>';
                    mevOutput += '<div class="uk-panel linkbox">';
                    mevOutput += '<p class="uk-text-small uk-margin-remove"><em>' + date + ' | ' + location + '</em></p>';
                    mevOutput += '<h4 class="uk-margin-remove"><a href="' + permalink + '" class="uk-link-reset">' + title + '</a></h4>';
                    mevOutput += '<p class="uk-text-small uk-margin-remove">' + introtext + '</p>';
                    mevOutput += '<hr class="uk-hidden@m">';
                    mevOutput += '</div>';
                    mevOutput += '</li>';
                }
                mevContainer.innerHTML = mevOutput;
            }
        })
        .catch(error => {
            mevContainer.innerHTML = '<p>[Technische Störung...]</p>';
            console.error('Fehler beim Laden der Termine:', error);
        });
</script>

                                                </ul>
<p data-uk-margin><a href="https://bienenkiste.de/event/" class="uk-button uk-button-secondary uk-button-small uk-border-pill"><span data-uk-icon="icon: calendar; ratio: 1"></span>&nbsp;Veranstaltungen</a></p>
                                            </div>
                                    </div>

                            </div>
