<script src="https://www.mellifera.de/js-pool/extern-content.js"></script>
<script>
   var ini    = 'bk';
   var type   = 'blog';
   var limit  = '6';
   var format = 'json';
   var formatdate = '%a %e. %b %Y';
   var ellipsis = '220';
   var imgsize = '550|360|30';

   
   var mecContainer = document.getElementById("mec-content");

   getMelliferaExternContent(ini,type,format,limit,formatdate,ellipsis,imgsize, function(results) {
      
      if ( results == 'error' ) {
         mecContainer.innerHTML = '<p>An error occurred.</p>';
      
      } else if ( results == 'empty' ) {
	 mecContainer.innerHTML = '<p>Sorry, no results found.</p>';
      
      } else {
         


      
      
         var mecOutput = '';
         var i;
         for(i = 0; i < results.length; i++) {
            var date        = results[i].date;
            var title       = results[i].pagetitle;
            var introtext   = results[i].introtext;
            var description = results[i].description;
            var permalink   = results[i].url;
            var image       = results[i].imageurl;
            var blog        = results[i].blog;
            var author      = results[i].author;
            
            mecOutput += '<li class="uk-width-4-5 uk-width-1-2@s uk-width-1-2@m uk-width-1-4@l uk-width-1-6@xxl">';
            mecOutput += '<article class="uk-panel linkbox"><div class="uk-card uk-card-default uk-card-small">';
            mecOutput += '<div class="uk-card-media-top"><img src="'  + image + '" alt="' + title + '"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 800 129.9" width="800" height="130"><path fill="#fff" stroke="none" d="M0,2.2C108.8-14,440.3,64.7,440.3,64.7S635.7,110.1,800,102v27.9H0V2.2z" /></svg></div>';
            mecOutput += '<div class="uk-card-body">';
            mecOutput += '<h3 class="uk-card-title">' + title + '</h3>';
            mecOutput += '<span class="uk-align-right"><img src="https://bienenkiste.de/images/' + blog + '_blog.svg" alt="Blog-Logo" style="width: 50px; height: 50px;"></span>';
            mecOutput += '<p class="uk-text-small uk-margin-remove"><span class="uk-article-meta"><span uk-icon="icon: star"></span> ' + date + '<br><span uk-icon="icon: user"></span> ' + author + '</span></p>';
            mecOutput += '<p class="uk-visible@s">' + introtext + '</p>';
            mecOutput += '<p class="uk-hidden@s">' + description + '</p>';
            mecOutput += '<a href="' + permalink + '" target="_blank">Weiterlesen <span uk-icon="icon: exit" title="externer Link"></span></a>';
            mecOutput += '</div>';
            mecOutput += '</div></article>';
            mecOutput += '</li>';
         }
	 mecOutput += '<li class="uk-width-4-5 uk-width-1-2@s uk-width-1-2@m uk-width-1-4@l uk-width-1-6@xxl"><article class="uk-panel linkbox"><div class="uk-card uk-card-primary uk-card-small"><div class="uk-card-media-top"><img src="https://bienenkiste.de/images/biki-14.jpg" alt=""><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 800 129.9" width="800" height="130"><path fill="#e99916" stroke="none" d="M0,2.2C108.8-14,440.3,64.7,440.3,64.7S635.7,110.1,800,102v27.9H0V2.2z" /></svg></div><div class="uk-card-body"><h3 class="uk-card-title">Weitere Beiträge</h3><p>Das <b>Bienenkisten-Blog</b> von Mellifera e.V.</p><a href="https://www.mellifera.de/blog/bienenkiste/?page=2" title="Weiterlesen" target="_blank">Weiterlesen <span uk-icon="icon: exit" title="externer Link"></span></a></a></div><div class="uk-card-footer"></div></div></article></li>';
         mecContainer.innerHTML = mecOutput;
      
      }
   });
</script>
