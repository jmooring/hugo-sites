<script type="text/javascript" src='https://code.jquery.com/jquery-3.3.1.min.js'></script>
<script type="text/javascript">

  $(function(){

    var map = window.localStorage.getItem('tree-section-state');

    if(map && map.length){
      map = map.split(',')
      $('.tree-section').each(function(){
        if(map.shift() == 'true') $(this).addClass('open')
      })
    }
    
    $('.tree-collapsible').show();

    $('.tree-section-header').click(function(){

      
      $(this).closest('.tree-section').toggleClass('open').find('.tree-section').removeClass('open');

      var map = [];
      $('.tree-section').each(function(){
        map.push($(this).hasClass('open'))
      })
      window.localStorage.setItem('tree-section-state',map)
    });
  });
</script>






