	$(document).ready(function(){
	  $('#slick1').slick({
		rows: 2,
		centerMode: true,
		autoplay: false,
		infinite: true,
		autoplaySpeed: 0, 
		cssEase: 'linear', 
		slidesToShow: 6,
		arrows: true,
		slidesToScroll: 1,
		responsive: [
			{
				breakpoint: 1200,
				settings: {
					slidesToShow: 6,
					infinite: true,
				}
			},
			{
				breakpoint: 992,
				settings: {
					slidesToShow: 5,
					infinite: true,
				}
			},
			{
				breakpoint: 768,
				settings: {
					slidesToShow: 4,
				}
			},
			{
				breakpoint: 576,
				settings: {
					slidesToShow: 3,
				}
			}
			
		]
	  });
	});
	</script>
<script>
	$(document).ready(function () {
		const lightboxOverlay = $('.lightbox-overlay');
		const lightboxContentText = $('.lightbox-content-text p');
		const lightboxContentAuthor = $('.lightbox-content-text .quote-author span');
		const lightboxContentLink = $('.lightbox-content-text .white-btn');
		const closeButton = $('.close-button');
		const navlinkButton = $('.nav-link');

		$('.slide-item.has-case-study').click(function () {
			const logo = $(this).find(".logo");
			const dataText = logo.data('text');
			const dataAuthor = logo.data('author');
			const dataLink = logo.data('link');

			lightboxContentText.html(dataText);
			lightboxContentAuthor.html(dataAuthor);

			
			if (dataLink) {
				lightboxContentLink.attr('href', dataLink).show();
			} else {
				lightboxContentLink.hide();
			}

			lightboxOverlay.fadeIn().css('display', 'flex');
			$('.tab-content').addClass('pause-scroll');
			$('.logo-grid').addClass('blur-scroll');
			$('html, body').animate({
				scrollTop: $("#pills-tabContent").offset().top - 95
			}, 300);
			$('.lightbox-content-text a:not([href])').addClass('hidden');
		});

		function closeLightbox() {
			lightboxOverlay.fadeOut().css('display', 'none');
			$('.tab-content').removeClass('pause-scroll');
		}

		closeButton.click(closeLightbox);
		navlinkButton.click(closeLightbox);
	});
</script>
<style>
.slick-wrapper-holder {
	justify-content: center;
}
.slick-wrapper {
	width: 100%;
}
.slide-item {
	background-color: #fff;
	color: #000;
	display: flex !important;
	justify-content: center;
	align-items: center;
	text-align: center;
	aspect-ratio: 1 / 1;
	margin-bottom: 20px;
	border: 1px solid white;
	box-shadow: 5px 5px 10px rgba(0, 0, 0, 0.25);
	position: relative;
	cursor: pointer;
}
.logo-section h3 {
	font-family: Lato, sans-serif;
	font-size: 40px;
}
.open-sign {
	position: absolute;
	right: 5%;
	bottom: 2%;
}
.logo-section {
	position: relative;
}
.slick-wrapper-holder .slide-item img {
	max-width: 90% !important;
	margin: auto;
}
 

.lightbox-overlay {
	display: none;
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background: #ffffff91;
	z-index: 999;
	justify-content: center;
	align-items: center;
	backdrop-filter: blur(6px);
}
 

