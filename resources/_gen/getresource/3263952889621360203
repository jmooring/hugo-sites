<script>
  
  function animatieLogoBlocks() {
    
    let logoBlocks = document.getElementsByClassName('logo-block');
    if(!logoBlocks) return;

    function callbackFunc(entries, observer)
    {
      entries.forEach(entry => {
        if(entry.isIntersecting) {
         entry.target.classList.remove('opacity-0', 'translate-y-12');
        }
      });
    }
    
    const options = {
        root: null,
        rootMargin: '0px',
        threshold: 0.3
      };
    
    let observer = new IntersectionObserver(callbackFunc, options);
    

    for (let element of logoBlocks) {
      observer.observe(element);
    };

  }

  animatieLogoBlocks();


  
  </script>













































<div id="it-has-never-been-so-easy" class='bg-white  '>
  <div class=' max-w-6xl  mx-auto    px-4 md:px-8 lg:px-12 py-6 md:py-12 lg:pt-20 '>
    <div class='  grid md:grid-cols-2 gap-6 md:gap-12 xl:gap-24 md:grid-flow-col-dense'>
      <div class=' md:col-start-2   flex flex-col self-center md:pb-8'>
        
        <h2 class="mb-4 text-2xl font-medium leading-9 text-gray-900 lg:mb-6 md:text-3xl">It has never been so easy</h2>
        
        
        
        
        
