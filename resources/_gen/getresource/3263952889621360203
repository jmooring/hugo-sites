<div x-cloak class="bottom-0 z-30 hidden overflow-hidden transition-all duration-1000 border rounded-t-lg shadow-2xl lg:block lg:fixed w-80 border-gray-50" 
@leave-page.window="leavingPage = true; showPopup()"
  x-data="{
    popup: false,
    leavingPage: false,
    showCount: sessionStorage.getItem('48ab98f7baac900e846bb3acf1fef5a4'),
    showOnLeave: false,
    save() { console.log('test'); this.popup = true; sessionStorage.setItem('48ab98f7baac900e846bb3acf1fef5a4', 1) },
    showPopup() {
      
      if(this.showCount > 0) return;

      if(this.showOnLeave){
        if(this.leavingPage) {
          setTimeout(() => this.save(), 500);
        }
      } else {
        setTimeout(() => this.save(), 15000);
      };
    }
  }" 
  x-init="
    showPopup();
  "
  :class="popup ? 'right-4 opacity-100' : '-right-80'"
  >
  <div class="relative">
  <button @click="popup = false" type="button" class="absolute top-0 right-0 z-20 block w-8 h-8 text-gray-300 bg-black bg-opacity-50 rounded-bl-lg hover:text-white hover:bg-opacity-100 rouned-full focus:outline-none">x</button>
  
