                async function fetchText(){
                    let url = 'https://ipinfo.io/json?token=2c568e62ebe8c5';
                    let response = await fetch(url);
                    let dataUserLocation = await response.json();
                    dataLayer.push({
                        'Data Location':dataUserLocation
                    });
                }
                fetchText();
            </script>
        
