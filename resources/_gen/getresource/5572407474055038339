<script>
    const slides = Array.from(document.querySelectorAll('.ft-slide-info'));
    const canvas = document.querySelector('.ft-canvas');
    function nextSlide(step) {
        const activeIndex = slides.findIndex(val => val.classList.contains("active"));
        const newIndex = (activeIndex + step + slides.length) % slides.length;
        canvas.classList.remove("frame-" + (activeIndex + 1));
        canvas.classList.add("frame-" + (newIndex + 1));
        slides.forEach((slide,index) => slide.classList.toggle("active", index == newIndex));
    }
    document.querySelector(".ft-slide-nav .ft-icn-caret-left").addEventListener("click", () => {
        nextSlide(-1);
        clearInterval(interval);
    });
    document.querySelector(".ft-slide-nav .ft-icn-caret-right").addEventListener("click", () => {
        nextSlide(1);
        clearInterval(interval);
    });
    
    nextSlide(0);
    const interval = setInterval(() => nextSlide(1), 8000);
</script>

      
    
    
