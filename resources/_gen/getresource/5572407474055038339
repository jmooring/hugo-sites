    <script>
    (function () {
        var modals = {
            "a[data-template='partner-signup-modal.html']": "dialog-partner",
            "a[data-template='contact-modal.html']": "dialog-contact",
            "a[data-template='get-started-modal.html']": "dialog-demo",
            "a[data-template='mobile-menu-modal.html']": "dialog-mobile-menu"
        }
        for (var modalSelector in modals){
            const dialogElement = document.getElementById(modals[modalSelector]);
            const bsModal = new bootstrap.Modal(dialogElement);
            const backButton = dialogElement.querySelector(".modal-back");

            function showView(selector) {
                dialogElement.querySelectorAll(".modal-view").forEach(x => x.classList.toggle("hide", !x.matches(selector)));
            }
            function resetViews() {                
                if(backButton) {
                    backButton.classList.add("hide");
                }
                showView('.initial-view');
            }
            
            
            document.querySelectorAll(modalSelector).forEach(x =>                 
                x.addEventListener("click", (e) => {
                    var wrappingModal = e.currentTarget.closest(".modal");
                    if(wrappingModal){
                        bootstrap.Modal.getInstance(wrappingModal).hide();
                    }
                    e.preventDefault();
                    bootstrap.Modal.getOrCreateInstance(dialogElement).show();
                }));

            
            dialogElement.querySelector(".modal-close").addEventListener("click", (e) => {
                const modal = e.currentTarget.closest(".modal");
                bootstrap.Modal.getInstance(modal).hide();
                resetViews();
            });

            
            dialogElement.querySelectorAll(".slide-navigation").forEach(x => x.addEventListener("click", (e) => {        
                showView(e.currentTarget.getAttribute("data-target-view"));
                
                const openModal = e.currentTarget.closest('.modal');
                openModal.querySelector('.modal-back').classList.remove("hide");
            }));
            
            
            if(backButton) {
                backButton.addEventListener("click", (e) => {
                    e.stopPropagation();
                    resetViews();
                });
            }

            
            dialogElement.querySelectorAll('form').forEach(x => x.addEventListener("submit", (e) =>{
                e.preventDefault();

                
                const errorAlert = e.target.querySelector('.form-error-alert');
                errorAlert.classList.add("hide");
                errorAlert.textContent = "";

                
                const inputs = e.target.querySelectorAll('[data-required]');
                inputs.forEach((input) => input.parentNode.classList.remove('field-error'));
                for(var input of inputs){
                    if(!input.value || input.value.trim() == ''){    
                        input.parentNode.classList.add('field-error');
                        if(errorAlert.classList.contains("hide")){
                            var name = input.getAttribute("data-name");
                            errorAlert.textContent = "We didn't understand the " + name + ", please correct it.";
                            errorAlert.classList.remove("hide");
                        }
                    }
                }
                
                
                if(!errorAlert.classList.contains("hide")) return;
                
                
                const data = new FormData(e.target);
                const entries = Object.fromEntries(data);
                entries["SourceUrl"] = window.location.href;
                const json = JSON.stringify(entries);

                
                fetch(e.target.action, { 
                    method: 'POST', 
                    body: json, 
                    headers: {
                        'Content-type': 'application/json; charset=UTF-8'
                    }})
                .then(function (res) {
                    if (res.ok) {
                        var modal = e.target.closest('.modal-body');
                        showView(".thank-you");
                        if(backButton) backButton.classList.add("hide");
                        e.target.reset();
                    } else {
                        errorAlert.textContent = res.status + ' ' + res.statusText;
                    }
                    errorAlert.classList.toggle("hide", res.ok);
                });
            }));
        }
        
    })();    
</script>

  </body>
</html>