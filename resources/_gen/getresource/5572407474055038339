<script>
    document.getElementById("fpd_notes_button").addEventListener("click", (e) => {
        e.target.classList.add("hide");
        document.getElementById("fpd_Notes").classList.remove("hide");
    });

    document.querySelectorAll("#section-full-platform-demo .dropdown-menu .dropdown-item").forEach(x => x.addEventListener("click", (e) => {
        const leadType = +e.currentTarget.parentNode.getAttribute("data-leadtype");
        
        if (leadType == 1)
            document.getElementById('representDropdown').textContent = "I represent a single property";
        else if (leadType == 2)
            document.getElementById('representDropdown').textContent = "I represent several properties";
        else if (leadType == 3)
            document.getElementById('representDropdown').textContent = "I represent a destination";
        else if (leadType == 4)
            document.getElementById('representDropdown').textContent = "I represent a marketing agency";
        else if (leadType == 99)
            document.getElementById('representDropdown').textContent = "I represent something else";

        
        document.getElementById("adr").classList.toggle("hide", leadType != 1 && leadType != 2);
        document.getElementById("adrLabelSingle").classList.toggle("hide", leadType != 1);
        document.getElementById("adrLabelMultiple").classList.toggle("hide", leadType != 2);
        document.getElementById("bookingEngineProvider").classList.toggle("hide", leadType != 1);
        document.getElementById('numberOfRooms').classList.toggle("hide", leadType != 1);
        document.getElementById('fpd_notes_button').classList.remove("hide");
        document.getElementById('lead-type').value = leadType;
    }))
</script>

