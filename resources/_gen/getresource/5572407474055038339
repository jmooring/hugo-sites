<script>
  let clickOutListener;
  const moreMenu = document.querySelector("#more-menu");
  const menu = document.querySelector(".ft-dropdown-menu");
  moreMenu.addEventListener("click", function moreMenuClicked(e) {
    menu.classList.add("show");
    
    clickOutListener = clickOutListener || document.addEventListener("click", function (e) {
      if (!e.target.matches('.ft-dropdown-menu *')) {
        menu.classList.remove("show");
        document.removeEventListener("click", clickOutListener);
        clickOutListener = null;
      }
    });
    e.stopPropagation();
  });
</script>

      
      
        
