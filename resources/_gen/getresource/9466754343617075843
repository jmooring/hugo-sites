				<div class="et_pb_code_inner"><script>
$(document).ready(function(){
    var $h2s = $('.fadeInOut-txt h2');
    var $mainH2 = $('.main-h2 h2');
    var index = 0;

    function updateText() {
        $mainH2.fadeOut(500, function() {
            // Update the text of the main h2
            $mainH2.text($h2s.eq(index).text());

            // Fade in with new text
            $mainH2.fadeIn(500);

            // Move to the next index
            index = (index + 1) % $h2s.length;
        });
    }

    // Start with the first h2 text
  //  $mainH2.text($h2s.eq(0).text());

    // Set interval to update text
    setInterval(updateText, 3000);
});


</script></div>
